[run]
source = .
omit = 
    tests/*
    test_*.py
    venv/*
    env/*
    */venv/*
    */env/*
    */site-packages/*
    setup.py
    compile_resources.py
    resources_rc.py
    ui_*.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml 